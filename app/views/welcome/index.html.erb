<% if user_signed_in? %>
    <h1>Welcome <%= current_user.email %></h1>
    <p>Find me in app/views/welcome/index.html.erb</p>
    <p>Hola Mundo</p>

    <% [1, 2, 3, 4, 5, 6].each do |number| %>
        <p>Number: <%= number %></p>
    <% end %>

    <% mundo = 'Uriel' %>
    <%= "Hola #{mundo}" %>
<% end %>